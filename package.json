{
  "name": "picpay-web",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.js",
  "dependencies": {
    "@material-ui/core": "3.8.1",
    "axios": "0.16.2",
    "dayjs": "1.7.7",
    "final-form": "4.10.0",
    "fingerprintjs2": "1.8.1",
    "lodash": "4.17.11",
    "polished": "2.2.0",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.5.2",
    "react-content-loader": "3.2.0",
    "react-custom-scrollbars": "4.2.1",
    "react-date-picker": "7.0.0",
    "react-dom": "16.5.2",
    "react-final-form": "3.6.6",
    "react-intl": "2.7.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-spring": "5.9.2",
    "react-test-renderer": "16.4.2",
    "recharts": "1.4.1",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-persist": "5.10.0",
    "redux-saga": "0.11.0",
    "redux-thunk": "2.2.0",
    "reselect": "2.5.3",
    "styled-components": "3.4.9",
    "styled-components-spacing": "3.1.0",
    "webpack-bundle-analyzer": "2.13.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.story.js",
      "!src/**/*.mock.js",
      "!src/**/__mock__/*.mock.js",
      "!src/**/__fixtures__/*.fixture.js",
      "!src/**/__helpers__/*",
      "!src/vendor/**/*",
      "!src/pages/**/*",
      "!src/translations/**/*",
      "!src/modules/index.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/store.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "proxy": "https://www.picpay.com",
  "homepage": "https://www.picpay.com/web",
  "scripts": {
    "start": "react-scripts start",
    "lint:js": "eslint ./src",
    "lint": "npm run lint:js",
    "precommit": "npm run lint && lint-staged",
    "prebuild": "npm run lint && npm run test",
    "build": "react-scripts build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:jest": "npm run lint",
    "test:coverage": "npm run test -- --coverage",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "build:all": "npm run build && npm run build-storybook",
    "analyze": "node analyzer"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.11",
    "@storybook/addon-links": "3.4.11",
    "@storybook/addon-viewport": "3.4.11",
    "@storybook/addons": "3.4.11",
    "@storybook/react": "3.4.11",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "cypress": "3.1.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.10.0",
    "eslint-plugin-prettier": "2.7.0",
    "husky": "0.14.3",
    "jest-localstorage-mock": "2.2.0",
    "jest-styled-components": "5.0.1",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "redux-mock-store": "1.5.3"
  }
}
